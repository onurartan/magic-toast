    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.13",
    "tsdx": "^0.14.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
